             temp = aiString(meshSrc.mShader);
             temp.Append("_d.tga");
             mat->AddProperty(&temp, AI_MATKEY_TEXTURE_DIFFUSE(0));
 
             temp = aiString(meshSrc.mShader);
             temp.Append("_h.tga");
             mat->AddProperty(&temp, AI_MATKEY_TEXTURE_HEIGHT(0));
 
