 		  : (UChar* )((void* )mem_end_stk[mem]));
 	  n = pend - pstart;
 	  DATA_ENSURE(n);
 	  sprev = s;
 	  swork = s;
 	  STRING_CMP_VALUE_IC(case_fold_flag, pstart, &swork, n, end, is_fail);
 	  if (is_fail) continue;
